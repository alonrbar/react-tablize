{
  "name": "react-tablize",
  "version": "0.0.0",
  "description": "React virtual table component",
  "main": "index.js",
  "author": "Alon Bar",
  "license": "MIT",
  "scripts": {
    "dev": "webpack --config webpack.dev.js --watch",
    "typecheck": "tsc --watch",
    "lint": "eslint \"./{src,test}/**/*.ts\" --exclude \"./**/*.d.ts\"",
    "test": "mocha-webpack --require source-map-support/register --webpack-config webpack.test.js \"test/**/*.tests.ts\"",
    "test-watch": "yarn test --watch",
    "release": "yarn lint && yarn test && webpack --config webpack.dev.js && webpack --config webpack.release.js"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/preset-typescript": "7.3.3",
    "@types/classnames": "^2.2.7",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.3",
    "@types/react-virtualized-auto-sizer": "1.0.0",
    "@types/react-window": "1.7.1",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "eslint": "5.16.0",
    "eslint-plugin-react": "7.12.4",
    "typescript": "3.4.3",
    "uglifyjs-webpack-plugin": "2.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2"
  }
}
